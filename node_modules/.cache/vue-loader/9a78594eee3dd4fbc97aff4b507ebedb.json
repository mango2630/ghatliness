-toggle=\"modal\" data-target=\"#layer\" v-on:click=\"nowIndex=$index\">X</button>\r\n\t\t\t\t</td>\r\n            </tr>\r\n\r\n            <tr v-show=\"myData.length==0\">\r\n                <td colspan=\"6\" class=\"text-center text-info\">\r\n                    <p>暂无数据...</p>\r\n                </td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <!-- end: add item progree -->\r\n\r\n    <!--弹出框-->\r\n    <div role=\"dialog\" class=\"modal fade\" id=\"layer\" data-index=\"{{nowIndex}}\">\r\n        <div class=\"modal-dialog\">\r\n            <div class=\"modal-content\">\r\n                <div class=\"modal-header\">\r\n                    <button class=\"close\" data-dismiss=\"modal\">\r\n                        <span>&times;</span>\r\n                    </button>\r\n                    <h4 class=\"modal-title\">确认删除吗？</h4>\r\n                </div>\r\n                <div class=\"modal-body text-right\">\r\n                    <button class=\"btn btn-cancel btn-sm\" data-dismiss=\"modal\">取消</button>\r\n                    <button class=\"btn btn-danger btn-sm\" data-dismiss=\"modal\" v-on:click=\"del(nowIndex)\">确认</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport '../../assets/promanage/js/bootstrap'\r\nimport \"../../assets/promanage/js/vue\"\r\n\r\nexport default {\r\n    name : 'ProManage',\r\n    data(){\r\n        return{\r\n            myData:[],\r\n            nowIndex: -100,\r\n            index : 0\r\n        }\r\n    },\r\n    methods:{\r\n        add:function(){\r\n            this.index ++;\r\n            this.myData.push({\r\n\r\n            })\r\n        },\r\n        del:function(n){\r\n            alert\r\n            if ( n== -2){\r\n                this.myData=\"\";\r\n            }else{\r\n                this.myData.splice(n, 1);\r\n            }\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import url(\".